{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "fideloper/proxy": "^4.0",
        "fidum/laravel-eloquent-morph-to-one": "^2.0",
        "illuminatech/validation-composite": "*",
        "laravel/framework": "^8.70",
        "laravel/telescope": "^4",
        "laravel/tinker": "^2.0",
        "livewire/livewire": "^2.0",
        "lorisleiva/laravel-actions": "^2.2",
        "spatie/laravel-enum": "^2.5",
        "staudenmeir/eloquent-has-many-deep": "^1.7",
        "vlucas/phpdotenv": "^5.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "doctrine/dbal": "^2.9",
        "fakerphp/faker": "^1.9.1",
        "filp/whoops": "^2.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "laravel/breeze": "^1.0",
        "laravel/dusk": "^6.12",
        "laravel/sail": "^1.10",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^5.0",
        "nunomaduro/larastan": "^0.7.12",
        "nunomaduro/laravel-mojito": "^0.2.4",
        "phpunit/phpunit": "^9",
        "squizlabs/php_codesniffer": "*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": ["laravel/telescope"]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "bootstrap/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
